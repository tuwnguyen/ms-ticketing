apiVersion: skaffold/v4beta11
kind: Config
build:
  artifacts:
    - image: tuwnguyendocker/auth
      context: auth
      sync:
        manual:
          - src: src/**/*.ts
            dest: .
      docker:
        dockerfile: Dockerfile
  local:
    push: false
manifests:
  rawYaml:
    - ./infra/k8s/*
deploy:
  kubectl: {}